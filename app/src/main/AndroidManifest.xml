<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.onlinetest.android.mofmapplayout">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MOFMAppLayout">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.MOFMAppLayout.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SettingsActivity"
            android:label="Settings">

        </activity>

        <activity
            android:name=".SelectSubjectActivity"
            android:label="Select Subject"
            android:theme="@style/Theme.MOFMAppLayout.NoActionBar">

        </activity>

        <activity
            android:name=".SelectTestActivity"
            android:label="Select Test"
            android:theme="@style/Theme.MOFMAppLayout.NoActionBar">>

        </activity>

    </application>

</manifest>